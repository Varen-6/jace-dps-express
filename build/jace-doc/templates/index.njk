{% extends "layout.njk" %}

{% block content %}
  
  <div class="markdown-body">
    
    <a href="javascript:history.back()" style="background-color: cornsilk;display: inline-block;padding: 8px 16px;border-radius: 10%;color: black;margin:25px;">
        Назад
    </a>
    <br/>

    
    
    {{ content | safe }}
  </div>
  
  <ul class="table-of-content">
    <div class="mt-5 text-black text-center w-5/6 mx-auto py-5 text-xl font-bold rounded bg-#ffd28e title">
           DPS Commands Help
         </div>
    {% for page in frontMatter.refs.items %}
      <li class="my-2 py-3 bg-white text-black rounded-xl text-center blinking">
        <a href="{{ page.url }}">
          {{ page.title }}
        </a>
      </li>
    {% endfor %}
  </ul>

{% endblock %}
